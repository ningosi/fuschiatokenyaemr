<?xml version="1.0" encoding="UTF-8"?>

<module configVersion="1.2">

	<!-- Module Properties -->
	<id>basicexample</id>
	<name>Basic Example Module</name>
	<version>1.0-SNAPSHOT</version>
	<package>org.openmrs.module.basicexample</package>
	<author>sd</author>
	<description>
		sd
	</description>
	<updateURL>https://modules.openmrs.org/modules/download/basicexample/update.rdf</updateURL>
	<require_version>1.9.8</require_version>
	<!-- / Module Properties -->

	
	<!-- Module Activator -->
	<activator>org.openmrs.module.basicexample.BasicExampleActivator</activator>

	<require_modules>
		<require_module version="${uiframeworkVersion}">org.openmrs.module.uiframework</require_module>
		<require_module version="2.2.1">org.openmrs.module.appframework</require_module>
		<require_module version="2.2">org.openmrs.module.kenyacore</require_module>
        <require_module version="2.4">org.openmrs.module.htmlformentry</require_module>
		<require_module version="2.6">org.openmrs.module.idgen</require_module>
	</require_modules>
	
	<!-- Adds link to admin page -->
	<extension>
		<point>org.openmrs.admin.list</point>
		<class>org.openmrs.module.basicexample.extension.html.AdminList</class>
	</extension>
	
	<!-- Maps hibernate file's, if present -->
	<mappingFiles>
				
	</mappingFiles>

	<!-- Internationalization -->
	<!-- All message codes should start with basicexample. -->
	<messages>
		<lang>en</lang>
		<file>messages.properties</file>
	</messages>
	<messages>
		<lang>fr</lang>
		<file>messages_fr.properties</file>
	</messages>
	<messages>
		<lang>es</lang>
		<file>messages_es.properties</file>
	</messages>
	<!-- /Internationalization -->

</module>

